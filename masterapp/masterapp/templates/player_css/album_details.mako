.albumsongtable {
    width: 100%;
    border-top: 1px solid ${c.profile.border};
}

.albumsongcell {
    width: 100%;
}

td.albumsong {
    border: none;
    height: 12pt;
    overflow: hidden;
    margin: 0pt;
    padding: 0pt;
}

td.albumart {
    text-align: center;
    vertical-align: middle;
    border: none;
    width: 80px;
}

img.albumart {
    border: 1px dashed ${c.profile.border};
}

