sudo rabbitmqctl add_vhost '/dev'
sudo rabbitmqctl set_permissions -p "/dev" guest ".*" ".*" ".*"

sudo rabbitmqctl add_vhost '/staging'
sudo rabbitmqctl set_permissions -p "/staging" guest ".*" ".*" ".*"

sudo rabbitmqctl add_vhost '/live'
sudo rabbitmqctl set_permissions -p "/live" guest ".*" ".*" ".*"
